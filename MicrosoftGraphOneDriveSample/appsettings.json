{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Services": {
    "WebApp": {
      "AuthEndpoint": "http://{0}/auth/access/?access_token={1}&refresh_token={2}"
    },
    "MS": {
      "Host": "login.microsoftonline.com",
      "AuthorizeEndpoint": "https://{0}/organizations/oauth2/v2.0/authorize?client_id={1}&response_type={2}&redirect_uri={3}&response_mode={4}&scope={5}&state={6}",
      "TokenEndpoint": "https://{0}/organizations/oauth2/v2.0/token",
      "ClientId": "{CLIENTE_ID}}",
      "ClientSecret": "{CLIENTE_SECRET}",
      "ResponseType": "code",
      "ResponseMode": "query",
      "RedirectUri": "http%3A%2F%2F{0}%2Fauth",
      "Scope": "user.read files.readWrite files.read.all files.readWrite.all files.readWrite.appFolder sites.readWrite.all directory.read.all",
      "State": "999999999999999",
      "GraphHost" : "https://graph.microsoft.com/v1.0"
    }
  }
}
